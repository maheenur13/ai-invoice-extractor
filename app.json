{
  "expo": {
    "name": "AI Receipt Scanner",
    "slug": "ai-invoice-extractor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ai-invoice-extractor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jahidun13.aiinvoiceextractor",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs camera access to capture receipt images for data extraction.",
        "NSPhotoLibraryUsageDescription": "This app needs photo library access to import receipt images for data extraction."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jahidun13.aiinvoiceextractor",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to capture receipt images."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to import receipt images."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to save receipt exports.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save exported files to your photos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c9541f72-5140-40fc-8ba9-a249aebcd9e4"
      }
    },
    "owner": "jahidun13",
    "updates": {
      "url": "https://u.expo.dev/c9541f72-5140-40fc-8ba9-a249aebcd9e4"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
